{
  "name": "SOCKS proxy",
  "description": "Configure a localhost SOCKS proxy",
  "homepage_url": "https://socksproxy.sova.st/",
  "author": "Hotplate Labs",
  "version": "0",
  "options_ui": {
    "chrome_style": true,
    "page": "options.html"
  },
  "permissions": [
    "contextMenus",
    "proxy",
    "storage"
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {},
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },  
  "manifest_version": 2
}
